<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.stage.*?>

<Stage xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.animu.Controller">

  <scene>
    <Scene>
      <AnchorPane prefHeight="390.0" prefWidth="550.0" style="-fx-background-color: #FFFFFF;">
            <children>
               <ImageView fitHeight="400.0" fitWidth="261.0" layoutX="8.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true">
                  <image>
                     <Image url="@../Imagenes/pantallaPrincipal.jpg" />
                  </image>
               </ImageView>
               <Label layoutX="250.0" layoutY="14.0" text="Registro">
                  <font>
                     <Font size="18.0" />
                  </font></Label>
               <Label layoutX="316.0" layoutY="82.0" text="Usuario" />
               <TextField fx:id="nuevoUsuario" layoutX="316.0" layoutY="113.0" />
               <Label layoutX="316.0" layoutY="153.0" text="Contraseña" />
               <PasswordField fx:id="nuevoPassword" layoutX="316.0" layoutY="181.0" />
               <Label layoutX="316.0" layoutY="221.0" text="Confirmar Contraseña" />
               <PasswordField fx:id="confirmarNuevoPassword" layoutX="316.0" layoutY="249.0" />
               <Button layoutX="390.0" layoutY="349.0" mnemonicParsing="false" onAction="#registrarse" style="-fx-background-color: #000000;" text="Registrarse" textFill="WHITE" />
               <Button fx:id="botonVolverRegistro" layoutX="320.0" layoutY="349.0" mnemonicParsing="false" onAction="#volverRegistro" style="-fx-background-color: #000000;" text="Volver" textFill="WHITE" />
            </children></AnchorPane>
    </Scene>
  </scene>
</Stage>
